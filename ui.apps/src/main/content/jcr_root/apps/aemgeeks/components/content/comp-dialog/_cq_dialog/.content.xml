                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        text="Author 1"
                                                        value="author1">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".checkbox-showhide-target"/>
                                                    </author1>
                                                    <author2
                                                        granite:class="cq-dialog-checkbox-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        text="Author 2"
                                                        value="author2">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".checkbox-showhide-target"/>
                                                    </author2>
                                                    <author3
                                                        granite:class="cq-dialog-checkbox-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        text="Author 3"
                                                        value="author3">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".checkbox-showhide-target"/>
                                                    </author3>
                                                    <author4
                                                        granite:class="cq-dialog-checkbox-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        text="Author 4"
                                                        value="author4">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".checkbox-showhide-target"/>
                                                    </author4>
                                                    <author5
                                                        granite:class="cq-dialog-checkbox-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        text="Author 5"
                                                        value="author5">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".checkbox-showhide-target"/>
                                                    </author5>
                                                    <author6
                                                        granite:class="cq-dialog-checkbox-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        text="Author 6"
                                                        value="author6">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".checkbox-showhide-target"/>
                                                    </author6>
                                                    <author7
                                                        granite:class="cq-dialog-checkbox-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        text="Author 7"
                                                        value="author7">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".checkbox-showhide-target"/>
                                                    </author7>
                                                    <author8
                                                        granite:class="cq-dialog-checkbox-showhide"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        text="Author 8"
                                                        value="author8">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-checkbox-showhide-target=".checkbox-showhide-target"/>
                                                    </author8>
