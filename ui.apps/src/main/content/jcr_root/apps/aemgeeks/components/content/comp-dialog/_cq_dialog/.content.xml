                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <fname
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="First Name"
                                                                name="./fname"/>
                                                            <lname
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Last Name"
                                                                name="./lname"
                                                                required="{Boolean}false"/>
                                                            <books
                                                                granite:class="geeks-multifield"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}false"
                                                                fieldLabel="Autor's Books">
                                                                <field
                                                                    granite:class="cmp-teaser__editor-action"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    name="./books"/>
                                                            </books>
                                                        </items>
                                                    </well>
                                                </items>
                                            </author3>
                                            <author4
                                                granite:class="hide checkbox-showhide-target"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhideradiovalue="author4"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <heading
                                                        granite:class="coral-Heading coral-Heading--4"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                                        level="{Long}4"
                                                        text="Author 4 : Author is Professor and Writer"/>
                                                    <well
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/well">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <subject
                                                                granite:class=""
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                fieldLabel="Professor's Subjects">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    name="./subjects"
                                                                    rootPath="/content"/>
                                                            </subject>
                                                            <books
                                                                granite:class="geeks-multifield"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                composite="{Boolean}false"
                                                                fieldLabel="Autor's Books">
                                                                <field
                                                                    granite:class="cmp-teaser__editor-action"
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    name="./books"/>
                                                            </books>
                                                        </items>
                                                    </well>
                                                </items>
                                            </author4>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
